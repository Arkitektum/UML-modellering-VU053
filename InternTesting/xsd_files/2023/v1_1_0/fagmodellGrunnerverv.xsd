  <simpleType name="ArealressursArealtypeType">
    <annotation>
      <documentation>AreaResourceAreaType: hovedinndeling etter kriterier for vegetasjon, naturlig drenering og kulturpåvirkning</documentation>
    </annotation>
    <union memberTypes="fmvrv:ArealressursArealtypeEnumerationType fmvrv:ArealressursArealtypeOtherType"/>
  </simpleType>
  <simpleType name="ArealressursArealtypeEnumerationType">
    <annotation>
      <documentation>AreaResourceAreaType: hovedinndeling etter kriterier for vegetasjon, naturlig drenering og kulturpåvirkning</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="11">
        <annotation>
          <documentation>Developed area: Bebygd; areal som er utbygd eller i betydelig grad opparbeida, samt tilstøtende arealer som i funksjon er nært knytta til bebyggelsen</documentation>
        </annotation>
      </enumeration>
      <enumeration value="81">
        <annotation>
          <documentation>Freshwater: Ferskvann; innsjø og elv</documentation>
        </annotation>
      </enumeration>
      <enumeration value="21">
        <annotation>
          <documentation>Fully cultivated land: Fulldyrka jord; jordbruksareal som er dyrka til vanlig pløyedybde, og som kan benyttes til åkervekster eller til eng, og som kan fornyes ved pløying</documentation>
        </annotation>
      </enumeration>
      <enumeration value="82">
        <annotation>
          <documentation>Ocean: Hav</documentation>
        </annotation>
      </enumeration>
      <enumeration value="99">
        <annotation>
          <documentation>Not surveyed: Ikke kartlagt; areal som har ukjent beskaffenhet.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="23">
        <annotation>
          <documentation>Home fields grazing land: Innmarksbeite; jordbruksareal som kan benyttes som beite, men som ikke kan høstes maskinelt. Minst 50 % av arealet skal være dekt av grasarter</documentation>
        </annotation>
      </enumeration>
      <enumeration value="60">
        <annotation>
          <documentation>Marsh: Myr; areal med myrvegetasjon og minst 30 cm tjukt torvlag</documentation>
        </annotation>
      </enumeration>
      <enumeration value="22">
        <annotation>
          <documentation>Superficially cultivated land: Overflatedyrka jord; jordbruksareal som for det meste er rydda og jevna i overflata, slik at maskinell høsting er mulig</documentation>
        </annotation>
      </enumeration>
      <enumeration value="12">
        <annotation>
          <documentation>Communications/traffic: Samferdsel; areal som brukes til samferdsel</documentation>
        </annotation>
      </enumeration>
      <enumeration value="30">
        <annotation>
          <documentation>Forest: Skog; areal med minst 6 trær per dekar som er eller kan bli 5 meter høye, og disse bør være jevnt fordelt på arealet.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="70">
        <annotation>
          <documentation>Snowy glacier??: Snøisbre; blanding av snø og isbre som ikke smelter i løpet av sommeren</documentation>
        </annotation>
      </enumeration>
      <enumeration value="50">
        <annotation>
          <documentation>Open firm ground: Åpen fastmark; fastmark som ikke er jordbruksareal, skog, bebygd eller samferdsel</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="ArealressursArealtypeOtherType">
    <restriction base="string">
      <pattern value="other: \w{2,}"/>
    </restriction>
  </simpleType>
